class MyStack {
public:
queue<int>q;
    MyStack() {
    }
    
    void push(int x) {
        q.push(x);
        for(int i=0;i<q.size()-1;i++)
        {
            int temp=q.front();
            q.pop();
            q.push(temp);

        }
        
    }
    
    int pop() {
         int val=q.front();
         q.pop();
         return val;
        
    }
    
    int top() {
        return q.front();
        
    }
    
    bool empty() {
        return q.empty();
        
    }
};
