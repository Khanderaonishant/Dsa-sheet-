// Online C++ compiler to run C++ program online
#include <iostream>
using namespace std;
char toLower(char ch)

{
    if(ch>'a' && ch<'z') return ch;
    else
    {
        char temp=ch-'A'+'a';
        return temp;
    }
}
int main() {
   char str;
   cout<<"enter string : "<<endl;
   cin>>str;
   cout<<"toLower  is : "<<toLower(str);

}
